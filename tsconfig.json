{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "pretty": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": [
      "esnext",
      "DOM",
      "ES6"
    ]
  }
}
